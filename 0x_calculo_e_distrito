   "source": [
    "import pandas as pd\n",
    "import geopandas as gpd\n",
    "import numpy as np\n",
    "from shapely.geometry import LineString\n",
    "from utils.save_shp import save_shp\n",
    "from utils.load_csv import load_csv\n",
    "from utils.load_shp import load_shp"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 113,
   "id": "d74a14b8",
   "metadata": {},
   "outputs": [],
   "source": [
    "df_posicoes = load_csv('df_posicoes.csv')\n",
    "distritos = load_shp(\"distritos.shp\")\n",
    "consumo_diesel = load_csv('consumo_diesel.csv')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 114,
   "id": "57f95769",
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_tipo_onibus(id_onibus):\n",
    "\n",
    "    onibus = df_posicoes[df_posicoes['id_onibus']==id_onibus]\n",
    "\n",
    "    return onibus['modelo'].unique()[0]\n",
    "\n",
    "def get_fator_consumo(tipo_veiculo):\n",
    "\n",
    "    cosumo_modelo = consumo_diesel[consumo_diesel['tecnologia']==tipo_veiculo]\n",
    "\n",
    "    return cosumo_modelo['com_ar_l_km'].unique()[0]\n",
    "\n",
    "def get_consumo_co2(row, km_rodados):\n",
    "\n",
    "    fator_emissao_co2 = 2.671 \n",
    "\n",
    "    consumo_l_km = row[\"com_ar_l_km\"] if row[\"tem_ar\"] else row[\"sem_ar_l_km\"]\n",
    "\n",
    "    emissao_co2 = km_rodados * consumo_l_km * fator_emissao_co2\n",
    "    \n",
    "    return emissao_co2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 115,
   "id": "7cde302b",
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_modelo_line(row):\n",
    "    return consumo_diesel.loc[consumo_diesel['tecnologia']==row['modelo']]  \n",
    "\n",
    "def get_consumo_l_km(row):\n",
    "    consumo_line = get_modelo_line(row)\n",
    "    consumo_l = consumo_line['com_ar_l_km'] if row['tem_ar'] else consumo_line['sem_ar_l_km']\n",
    "    consumo_value = row['distancia'] * consumo_l\n",
    "    return consumo_value\n",
    "\n",
    "def get_consumo_kg_km(row):\n",
    "    consumo_line = get_modelo_line(row)\n",
    "    consumo_kg = consumo_line['com_ar_kg_km'] if row['tem_ar'] else consumo_line['sem_ar_kg_km']\n",
    "    consumo_value = row['distancia'] * consumo_kg\n",
    "    return consumo_value"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 116,
   "id": "f961c97f",
   "metadata": {},
   "outputs": [],
   "source": [
    "#transformar em gdf + epsg:31983\n",
    "gdf = gpd.GeoDataFrame(df_posicoes, geometry=gpd.points_from_xy(df_posicoes.lon, df_posicoes.lat), crs=\"EPSG:4326\")\n",
    "\n",
    "gdf = gdf.to_crs(\"EPSG:31983\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 117,
   "id": "c416827c",
   "metadata": {},
   "outputs": [],
   "source": [
    "#dropar pontos únicos (ônibus que só tem 1 ponto)\n",
    "dropar_filter = (\n",
    "    gdf['id_onibus']\n",
    "    .value_counts()\n",
    "    [(gdf['id_onibus'].value_counts() == 1)]\n",
    "    .index\n",
    ")\n",
    "\n",
    "gdf = gdf[~gdf['id_onibus'].isin(dropar_filter)].reset_index(drop=True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 118,
   "id": "8dd6515a",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Criar linestrings\n",
    "trajetos = (\n",
    "    gdf\n",
    "    .groupby(\"id_onibus\")[\"geometry\"]\n",
    "    .apply(lambda x: LineString(x.tolist()))\n",
    ")\n",
    "trajetos = gpd.GeoDataFrame(\n",
    "    trajetos, \n",
    "    geometry=\"geometry\", \n",
    "    crs=\"EPSG:31983\"\n",
    ").reset_index()\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 119,
   "id": "324c085b",
   "metadata": {},
   "outputs": [],
   "source": [
    "#overlay\n",
    "gdf_overlay = gpd.overlay(trajetos, distritos, how=\"intersection\")\n",
    "\n",
    "gdf_overlay['distancia'] = gdf_overlay.geometry.length / 1000 #distancia em km\n",
    "\n",
    "#gdf_overlay.explore()"
   ]
  },
