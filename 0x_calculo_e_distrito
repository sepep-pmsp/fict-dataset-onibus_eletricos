   "outputs": [],
   "source": [
    "#acho que não precisa fazer isso agora agora, pode ser depois\n",
    "gdf_overlay = gdf_overlay.merge(\n",
    "    consumo_diesel, \n",
    "    left_on=\"modelo\", \n",
    "    right_on=\"tecnologia\", \n",
    "    how=\"left\"\n",
    ")\n",
    "\n",
    "## aleatorização do ar condicionado\n",
    "np.random.seed(42)\n",
    "gdf_overlay[\"tem_ar\"] = np.random.choice([True, False], size=len(gdf_overlay))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 125,
   "id": "508037c8",
   "metadata": {},
   "outputs": [],
   "source": [
    "gdf_overlay['consumo_l_diesel'] = gdf_overlay['distancia'] * np.where(\n",
    "    gdf_overlay['tem_ar'], #bool\n",
    "    gdf_overlay['com_ar_l_km'], #valor se True\n",
    "    gdf_overlay['sem_ar_l_km'] #valor se False\n",
    ")\n",
    "\n",
    "gdf_overlay['consumo_kg_diesel'] = gdf_overlay['distancia'] * np.where(\n",
    "    gdf_overlay['tem_ar'], #bool\n",
    "    gdf_overlay['com_ar_kg_km'], #valor se True\n",
    "    gdf_overlay['sem_ar_kg_km'] #valor se False\n",
    ")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 126,
   "id": "fe224f87",
   "metadata": {},
   "outputs": [],
   "source": [
    "gdf_overlay['emissao_co2'] = gdf_overlay['consumo_l_diesel'] * 2.671 #fator de emissão de CO2 do diesel"
   ]
  },
